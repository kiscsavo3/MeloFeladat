@model MvcRepules.BLL.DTO.LogUploadDto
@{
    ViewData["Title"] = "LogUpload";
}



<form asp-controller="Upload" asp-action="LogUploaded" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload log file:</p>
            <input type="file" name="LogFile">
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload">
        </div>
    </div>
</form>

