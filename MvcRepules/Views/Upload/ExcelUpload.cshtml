@model  MvcRepules.BLL.DTO.ExcelUploadDto
@{
    ViewData["Title"] = "ExcelUpload";
}

<form asp-controller="Upload" asp-action="ExcelUploaded" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload excel file:</p>
            <input type="file" name="ExcelFile">
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload">
        </div>
    </div>
</form>